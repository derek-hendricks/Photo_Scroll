<% provide(:title, "Latest Photos") %>
<div class="page-header">
  <h1>Latest Photos</h1>
</div>

<div id="static-pagination">
  <%= will_paginate %>
</div>

<div id="my-posts">
  <div class="page-delimiter first-page" data-page="<%= params[:page] || 1 %>"></div>
    <%= render @messages %>
</div>
    
<% if @messages.next_page %>
  <div id="infinite-scrolling">
    <%= will_paginate %>
  </div>
<% end %>